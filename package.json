{
	"name": "mcbewrs",
	"version": "0.0.0",
	"scripts": {
		"postinstall": "bower install",
		"start": "export NODE_ENV=development && nodemon ./server.js",
		"start-production": "node ./server.js",
		"format": "prettier --write .",
		"push": "git update-index --assume-unchanged data/* && git push"
	},
	"dependencies": {
		"@prettier/plugin-pug": "^1.10.1",
		"bower": "^1.8.8",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"csurf": "^1.11.0",
		"debug": "~2.6.9",
		"express": "~4.16.1",
		"express-minify": "^1.0.0",
		"express-rate-limit": "^5.1.3",
		"express-session": "^1.17.1",
		"http-errors": "~1.6.3",
		"js-yaml": "^3.14.0",
		"jsonwebtoken": "^8.5.1",
		"morgan": "~1.9.1",
		"nodemon": "^2.0.4",
		"pbkdf2-password": "^1.2.1",
		"prettier": "^2.1.2",
		"pug": "^3.0.0",
		"sass": "^1.29.0",
		"spdy": "^4.0.2",
		"sqlite3": "^5.0.0",
		"tomlify-j0.4": "^3.0.0",
		"xml": "^1.0.1"
	},
	"prettier": {
		"useTabs": true
	},
	"nodemonConfig": {
		"verbose": true,
		"ignore": [
			"data/*",
			"public/*",
			".git/*",
			".github/*",
			"cache/*",
			"docs/*"
		]
	}
}
