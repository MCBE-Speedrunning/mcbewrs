doctype html
html(lang="en")
	head
		title MCBE WR History
		meta(charset="utf-8")/
		meta(name="viewport", content="width=device-width, initial-scale=1.0")/
		meta(property="og:title", content=pageName)/
		link(rel="manifest", href="/cdn/manifest.json")/
		link(rel="stylesheet", href="/cdn/stylesheets/style.css")/
		link(
			rel="stylesheet",
			href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-alpha3/dist/css/bootstrap.min.css",
			crossorigin="anonymous",
			integrity="sha384-CuOF+2SnTUfTwSZjCXf01h7uYhfOBuxIhGKPbfEJ3+FqH/s6cIFN9bGr1HmAg4fQ"
		)/

		#colorScheme
			link(rel="stylesheet", type="text/css", href="/cdn/stylesheets/dark.css", media="(prefers-color-scheme: dark)")/
			link(rel="stylesheet", type="text/css", href="/cdn/stylesheets/light.css", media="(prefers-color-scheme: light)")/
	body
		.alert.alert-danger.alert-dismissible.fade.show(role="alert") This website is in beta
			button.close(type="button", data-dismiss="alert", aria-label="Close")
				span(aria-hidden="true") &times;

		if banner
			.alert.alert-dismissible.fade.show(class=`alert-${banner.status}`, role="alert")= banner.text
				button.close(type="button", data-dismiss="alert", aria-label="Close")
					span(aria-hidden="true") &times;

		include nav.pug
		.container
			block content

		script(src="/cdn/javascripts/main.js", defer="")
		script(
			src="https://twemoji.maxcdn.com/v/latest/twemoji.min.js",
			crossorigin="anonymous"
		)
		script(src="/cdn/components/Keypress/keypress.js")
		script(
			src="https://code.jquery.com/jquery-3.5.1.slim.min.js",
			integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj",
			crossorigin="anonymous"
		)

		script(
			src="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/js/bootstrap.min.js",
			crossorigin="anonymous",
			integrity="sha384-w1Q4orYjBQndcko6MimVbzY0tgp4pWB4lZ7lr30WKz0vr/aWKhXdBNmNb5D92v7s"
		)
